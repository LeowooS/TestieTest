<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="联系人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['addressData']]]]]}}" value="{{addressData.name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="手机号" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">街道</text><input class="input" type="text" placeholder="街道" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','addressName','$event',[]],['addressData']]]]]}}" value="{{addressData.addressName}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">门牌号</text><input class="input" type="text" placeholder="门牌号" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','postNumber','$event',[]],['addressData']]]]]}}" value="{{addressData.postNumber}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">城市</text><input class="input" type="text" placeholder="城市" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','city','$event',[]],['addressData']]]]]}}" value="{{addressData.city}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.defaule}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>