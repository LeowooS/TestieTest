<view class="container"><view class="mp-search-box"><input class="ser-input" type="text" value="输入关键字搜索" disabled="{{true}}"/></view><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background" style="{{'background-color:'+(titleNViewBackground)+';'}}"></view><swiper class="carousel" circular="{{true}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{carouselList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['navToDetailPage',[['o',['title','advertising']]]]]]]}}" class="carousel-item" bindtap="__e"><image src="{{item.src}}"></image></swiper-item></block></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><view class="cate-section"><block wx:for="{{category}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cate-item"><image src="{{item.image}}"></image><text>{{item.name}}</text></view></block></view><view class="ad-1"><image src="/static/temp/ad1.jpg" mode="scaleToFill"></image></view><view class="guess-section"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="guess-item" bindtap="__e"><view class="image-wrapper"><image src="{{item.image}}" mode="aspectFill"></image></view><text class="title clamp">{{item.title}}</text><text class="price">{{"$"+item.price}}</text></view></block></view></view>